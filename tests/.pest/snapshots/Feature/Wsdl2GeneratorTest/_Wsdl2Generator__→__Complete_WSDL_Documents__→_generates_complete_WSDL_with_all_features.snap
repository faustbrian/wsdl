<?xml version="1.0" encoding="UTF-8"?>
<wsdl:description xmlns:wsdl="http://www.w3.org/ns/wsdl" xmlns:tns="http://example.com/complete" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:wsoap="http://www.w3.org/ns/wsdl/soap" targetNamespace="http://example.com/complete">
  <wsdl:documentation xml:lang="en">Complete WSDL 2.0 demonstration</wsdl:documentation>
  <wsdl:types>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://example.com/complete">
      <xs:import namespace="http://schemas.example.com/common" schemaLocation="common.xsd"/>
      <xs:simpleType name="Status">
        <xs:restriction base="xs:string">
          <xs:enumeration value="active"/>
          <xs:enumeration value="inactive"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="User">
        <xs:sequence>
          <xs:element name="id" type="xs:int"/>
          <xs:element name="name" type="xs:string"/>
          <xs:element name="email" type="xs:string"/>
          <xs:element name="status" type="tns:Status"/>
        </xs:sequence>
      </xs:complexType>
    </xs:schema>
  </wsdl:types>
  <wsdl:interface name="UserInterface">
    <wsdl:documentation xml:lang="en">User management interface</wsdl:documentation>
    <wsdl:fault name="InvalidUserFault" element="tns:InvalidUserError"/>
    <wsdl:operation name="GetUser" pattern="http://www.w3.org/ns/wsdl/in-out" safe="true">
      <wsdl:documentation xml:lang="en">Retrieve user by ID</wsdl:documentation>
      <wsdl:input element="tns:GetUserRequest"/>
      <wsdl:output element="tns:GetUserResponse"/>
      <wsdl:outfault ref="tns:InvalidUserFault"/>
    </wsdl:operation>
    <wsdl:operation name="CreateUser" pattern="http://www.w3.org/ns/wsdl/in-out">
      <wsdl:documentation xml:lang="en">Create new user</wsdl:documentation>
      <wsdl:input element="tns:CreateUserRequest"/>
      <wsdl:output element="tns:CreateUserResponse"/>
    </wsdl:operation>
  </wsdl:interface>
  <wsdl:binding name="UserBinding" interface="tns:UserInterface" type="http://www.w3.org/ns/wsdl/soap">
    <wsdl:documentation xml:lang="en">SOAP 1.2 binding</wsdl:documentation>
    <wsdl:operation xmlns:wsoap="http://www.w3.org/ns/wsdl/soap" ref="tns:GetUser">
      <wsoap:operation xmlns:wsoap="http://www.w3.org/ns/wsdl/soap" soapAction="http://example.com/GetUser"/>
    </wsdl:operation>
    <wsdl:operation xmlns:wsoap="http://www.w3.org/ns/wsdl/soap" ref="tns:CreateUser">
      <wsoap:operation xmlns:wsoap="http://www.w3.org/ns/wsdl/soap" soapAction="http://example.com/CreateUser"/>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="UserService" interface="tns:UserInterface">
    <wsdl:documentation xml:lang="en">User management service</wsdl:documentation>
    <wsdl:endpoint name="UserEndpoint" binding="tns:UserBinding" address="http://example.com/soap">
      <wsdl:documentation xml:lang="en">Primary SOAP endpoint</wsdl:documentation>
    </wsdl:endpoint>
  </wsdl:service>
</wsdl:description>
