<?xml version="1.0" encoding="UTF-8"?>
<wsdl:description xmlns:wsdl="http://www.w3.org/ns/wsdl" xmlns:tns="http://example.com/groups" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:wsoap="http://www.w3.org/ns/wsdl/soap" targetNamespace="http://example.com/groups">
  <wsdl:types>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://example.com/groups">
      <xs:group name="AddressFields">
        <xs:sequence>
          <xs:element name="street" type="xs:string"/>
          <xs:element name="city" type="xs:string"/>
          <xs:element name="country" type="xs:string"/>
        </xs:sequence>
      </xs:group>
      <xs:attributeGroup name="AuditAttributes">
        <xs:attribute name="createdAt" type="xs:dateTime"/>
        <xs:attribute name="updatedAt" type="xs:dateTime"/>
      </xs:attributeGroup>
      <xs:complexType name="Contact">
        <xs:sequence>
          <xs:element name="name" type="xs:string"/>
          <xs:group ref="tns:AddressFields"/>
        </xs:sequence>
        <xs:attributeGroup ref="tns:AuditAttributes"/>
      </xs:complexType>
    </xs:schema>
  </wsdl:types>
</wsdl:description>
