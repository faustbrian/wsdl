<?xml version="1.0" encoding="UTF-8"?>
<wsdl:description xmlns:wsdl="http://www.w3.org/ns/wsdl" xmlns:tns="http://example.com/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:wsoap="http://www.w3.org/ns/wsdl/soap" targetNamespace="http://example.com/">
  <wsdl:interface name="Interface">
    <wsdl:fault name="Fault1" element="tns:Error1"/>
    <wsdl:fault name="Fault2" element="tns:Error2"/>
    <wsdl:operation name="FaultyOp">
      <wsdl:input element="tns:Request"/>
      <wsdl:output element="tns:Response"/>
      <wsdl:outfault ref="tns:Fault1"/>
      <wsdl:outfault ref="tns:Fault2"/>
    </wsdl:operation>
  </wsdl:interface>
</wsdl:description>
