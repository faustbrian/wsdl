<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://example.com/snapshot" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsp="http://www.w3.org/ns/ws-policy" xmlns:wsaw="http://www.w3.org/2006/05/addressing/wsdl" xmlns:default="http://example.com/policies" name="SnapshotService" targetNamespace="http://example.com/snapshot">
  <wsp:Policy xmlns:default="http://example.com/policies" xml:id="SnapshotPolicy" Name="SnapshotPolicyName">
    <wsp:All xmlns:default="http://example.com/policies">
      <default:CustomAssertion xmlns="http://example.com/policies"/>
    </wsp:All>
  </wsp:Policy>
  <wsdl:types>
    <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://example.com/snapshot">
      <xsd:complexType name="OperationRequest">
        <xsd:sequence>
          <xsd:element name="id" type="xsd:int"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OperationResponse">
        <xsd:sequence>
          <xsd:element name="status" type="xsd:string"/>
        </xsd:sequence>
      </xsd:complexType>
    </xsd:schema>
  </wsdl:types>
  <wsdl:message name="OperationInput">
    <wsdl:part name="parameters" element="tns:OperationRequest"/>
  </wsdl:message>
  <wsdl:message name="OperationOutput">
    <wsdl:part name="parameters" element="tns:OperationResponse"/>
  </wsdl:message>
  <wsdl:portType name="SnapshotServicePortType" wsaw:UsingAddressing="true">
    <wsdl:operation name="Execute">
      <wsdl:input message="tns:OperationInput">
        <wsaw:Action>http://example.com/Execute</wsaw:Action>
      </wsdl:input>
      <wsdl:output message="tns:OperationOutput">
        <wsaw:Action>http://example.com/ExecuteResponse</wsaw:Action>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name="SnapshotServiceBinding" type="tns:SnapshotServicePortType">
    <wsp:PolicyReference xmlns:wsp="http://www.w3.org/ns/ws-policy" URI="#SnapshotPolicy"/>
    <soap:binding xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" name="Execute">
      <soap:operation xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" soapAction="urn:Execute" style="document"/>
      <wsdl:input xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
        <soap:body xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" use="literal"/>
      </wsdl:input>
      <wsdl:output xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
        <soap:body xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="SnapshotService">
    <wsdl:port xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" name="SnapshotServicePort" binding="tns:SnapshotServiceBinding">
      <soap:address xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" location="http://example.com/soap/snapshot"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
